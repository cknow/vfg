{
  "name": "@cknow/vfg",
  "version": "0.0.0-semantically-released",
  "description": "A schema-based form generator component for Vue.js",
  "keywords": [
    "vue",
    "vuejs",
    "vfg",
    "form",
    "generator",
    "schema",
    "json"
  ],
  "homepage": "https://github.com/cknow/vfg",
  "bugs": {
    "url": "https://github.com/cknow/vfg/issues",
    "email": "ricardo@clicknow.com.br"
  },
  "license": "MIT",
  "author": {
    "name": "Ricardo Gobbo de Souza",
    "email": "ricardogobbosouza@yahoo.com.br",
    "role": "Developer"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/vfg.common.js",
  "module": "dist/vfg.esm.js",
  "browser": "dist/vfg.js",
  "unpkg": "dist/vfg.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cknow/vfg"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "commit": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coverage": "npm t -- --coverage",
    "lint": "eslint src/ test/ --ext .js,.vue",
    "posttest": "npm run build",
    "prebuild": "rimraf dist/",
    "precommit": "npm t",
    "precoverage": "rimraf coverage/",
    "prepush": "npm run coverage",
    "pretest": "npm run lint",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "test": "exit 0",
    "watch": "npm t -- --watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@cknow/eslint-config": "^2.12.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.5.0",
    "husky": "^0.14.3",
    "jest": "^23.0.1",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-vue": "^4.2.0",
    "vue": "^2.5.16",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.16"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
